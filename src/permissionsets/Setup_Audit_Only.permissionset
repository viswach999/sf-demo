    <description>Recipients of this permission set are able to set audit fields when using an API dataloader</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Setup Audit Only</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>CreateAuditFields</name>
    </userPermissions>
</PermissionSet>
