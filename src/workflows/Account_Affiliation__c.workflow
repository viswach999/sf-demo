    <rules>
        <fullName>Account Affiliation Created</fullName>
        <actions>
            <name>BW_Account_Affiliation_Created</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>DATEVALUE(CreatedDate)  =  TODAY()</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Account Affiliation Deactivate</fullName>
        <actions>
            <name>BW_Account_Affiliation_Deleted</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>This will deactivate send outbound message when isActive = false</description>
        <formula>AND(
 NOT(ISNEW()),
 IsActive__c = False
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Account Affiliation Updated</fullName>
        <actions>
            <name>BW_Account_Affiliation_Updated</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>NOT(ISNEW()) 
/* comment out this code -- only needed for initial deployment
&amp;&amp; 
AND( 
NOT(Account_Record_Type_Name__c=&quot;Business&quot;) 
,NOT(Account_Record_Type_Name__c=&quot;School&quot;) 
)
*/</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Acct Affil Deactivate</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Account_Affiliation__c.End_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>This will deactivate (IsActive__c will be false) when End_Date__c = TODAY()</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Acct_Affil_Deactivate</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Account_Affiliation__c.End_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Chapter Approval of Transition_AA</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Account_Affiliation__c.Chapter_Approval_Status__c</field>
            <operation>equals</operation>
            <value>Pending</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account_Affiliation__c.Account_Record_Type_Name__c</field>
            <operation>equals</operation>
            <value>Chapter</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account_Affiliation__c.Transfer_Flag__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Account_Affiliation__c.Contact_Type__c</field>
            <operation>equals</operation>
            <value>Member</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Chapter_Approval_of_Transition_Account_Affiliation</name>
                <type>Alert</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Make Account Affiliation Inactive</fullName>
        <active>true</active>
        <formula>and(NOT(isnull(End_Date__c)),NOT(RecordType.Name = &quot;Business&quot;))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Make_Is_Active_False</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Account_Affiliation__c.End_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Profile Complete Update</fullName>
        <actions>
            <name>Profile_Complete_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account_Affiliation__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Business</value>
        </criteriaItems>
        <description>Updates the completion of profile on contact based on fields being filled out</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <tasks>
        <fullName>Send_Email_to_Chapter_for_Transfer_Approval_Sent</fullName>
        <assignedTo>datamigr@ypowpo.org</assignedTo>
        <assignedToType>user</assignedToType>
        <dueDateOffset>0</dueDateOffset>
        <notifyAssignee>false</notifyAssignee>
        <priority>Normal</priority>
        <protected>false</protected>
        <status>Completed</status>
        <subject>Send Email to Chapter for Transfer Approval - Sent</subject>
    </tasks>
</Workflow>
