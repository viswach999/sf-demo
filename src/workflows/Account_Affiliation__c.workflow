    <alerts>
        <fullName>Chapter_Approval_of_Transition_Account_Affiliation</fullName>
        <description>Chapter Approval of Transition - Account Affiliation</description>
        <protected>false</protected>
        <recipients>
            <field>Chapter_Administrator__c</field>
            <type>userLookup</type>
        </recipients>
        <recipients>
            <field>Chapter_Chair__c</field>
            <type>userLookup</type>
        </recipients>
        <recipients>
            <field>Chapter_Membership_Officer__c</field>
            <type>userLookup</type>
        </recipients>
        <senderAddress>membership@ypo.org</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>unfiled$public/Chapter_Approval_of_Transition_Account_Affiliation</template>
    </alerts>
    <fieldUpdates>
        <fullName>Acct_Affil_Deactivate</fullName>
        <description>Deactivates Account Affiliation when End_Date__c = TODAY()</description>
        <field>IsActive__c</field>
        <literalValue>0</literalValue>
        <name>Acct Affil Deactivate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Make_Is_Active_False</fullName>
        <field>IsActive__c</field>
        <literalValue>0</literalValue>
        <name>Make Is Active False</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Profile_Complete_Update</fullName>
        <field>Business_Profile_Complete__c</field>
        <formula>IF(IsActive__c = TRUE, 
	(
	IF(NOT(ISNULL(Account__c)), 1, 0) + 
	IF(NOT(ISPICKVAL(Industry_Category_1__c, &quot;&quot;)), 1, 0) +
	IF(NOT(ISBLANK(Description__c)), 1, 0) +
	IF(NOT(ISPICKVAL(Job_Title__c, &quot;&quot;)), 1, 0) +
	IF(NOT(ISPICKVAL(Organization_Type__c, &quot;&quot;)), 1, 0)
	),
	0
)</formula>
        <name>Profile Complete Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <targetObject>Contact__c</targetObject>
    </fieldUpdates>
